{
    "version": 2,
    "builds": [{ "src": "app.js", "use": "@vercel/node" }],
    "routes": [
      {
        "src": "/store",
        "dest": "/routes/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/app.js"
      }
    ],
    "rewrites": [
      { "source": "/store", "destination": "/routes/index.js" },
      { "source": "/(.*)", "destination": "/app.js" }
    ]
  }
  